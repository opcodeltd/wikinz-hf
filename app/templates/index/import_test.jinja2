{%- set page_title = 'Home' -%}
{%- extends 'base.jinja2' %}
{%- block content -%}
    <div class="row-fluid">
        <div class="col-md-6">
            <h3>Input</h3>
            <table class="table">
                <thead>
                    <tr>
                        <th>(A) A date</th>
                        <th>(B) A time</th>
                        <th>(C) Some NZD</th>
                        <th>(D) Something else</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>&nbsp;</td>
                    </tr>
                                        <tr>
                        <td>&nbsp;</td>
                    </tr>
                    {% for i in range(0,30) %}
                    <tr>
                        <td>1/1/2010</td>
                        <td>11:02</td>
                        <td>20</td>
                        <td>Seven ducks</td>
                    </tr>
                    {% endfor %}
                </tbody>
            </table>
        </div>
        <div class="col-md-6">
            <h3>Output</h3>
            <table class="table">
                <thead>
                    <tr>
                        <th>Date/Time</th>
                        <th>Value of Cat</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td><select>
                            <option value="">A</option>
                            <option value="">B</option>
                            <option value="" selected="selected">A+B</option>
                        </select></td>
                        <td><select>
                            <option value="">A</option>
                            <option value="" selected="selected">B</option>
                        </select></td>
                    </tr>
                    <tr>
                        <td><select>
                            <option value="">Date Time</option>
                            <option value="">Number</option>
                            <option value="">String</option>
                        </select></td>
                        <td><select>
                            <option value="">Date Time</option>
                            <option value="" selected="selected">Number</option>
                            <option value="">String</option>
                        </select></td>
                    </tr>
                    {% for i in range(0,30) %}
                    <tr>
                        <td>2010-01-01 11:02</td>
                        <td>10.00</td>
                    </tr>
                    {% endfor %}
                </tbody>
            </table>
        </div>
    </div>
{%- endblock -%}
{% block script %}
    <script src="http://fb.me/react-0.11.2.js"></script>
    <script type="text/jsx" src="{{ cdn('js/sheet.js') }}"></script>
{% endblock %}
